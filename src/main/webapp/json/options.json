                { 
                    "id": "parameter_al",
                    "description": "Create your own lipid",
                    "input_type": "multitext",
                    "default": [["", "", "", ""]],
                    "information": "Create your own lipids by defining your head groups, linker groups and tail groups; separated by spaces. Relative abundance is optional (see: Membrane lipids). Head (optional): possible groups are C, E, G, S, P. Linker (required): possible groups are A, G. Tail (required): groups can consist of multiple tail beads, possible tail beads are C, D, T. The amount of tails must be equal to the amount of linkers.",
                    "class": "advanced",
                    "placeholders": [
                        "head e.g. 'C E G S P'",
                        "linker e.g. 'A G'",
                        "tail e.g. 'CCC CDT'",
                        "ratio e.g. 4:1"
                    ]
                },
                { 
                    "id": "parameter_rand",
                    "description": "Membrane random kick",
                    "input_type": "number",
                    "min": "",
                    "max": "",
                    "step": "0.1",
                    "default": "",
                    "placeholder": "e.g. 2",
                    "information": "Gives the membrane lipids a random kick, so they appear more natural.",
                    "class": "advanced"
                },
                { 
                    "id": "parameter_a",
                    "description": "Area per lipid",
                    "input_type": "number",
                    "min": "0.1",
                    "max": "",
                    "step": "0.1",
                    "default": "",
                    "placeholder": "e.g. 0.6",
                    "information": "Area per lipid in nm * nm. This parameter is used for both leaflets, unless 'Area per lipid in upper leaflet' is given. The default value is 0.6 nm.",
                    "class": "advanced"
                },
                { 
                    "id": "parameter_au",
                    "description": "Area per lipid in upper leaflet",
                    "input_type": "number",
                    "min": "0.1",
                    "max": "",
                    "step": "0.1",
                    "default": "",
                    "placeholder": "e.g. 0.6",
                    "information": "Area per lipid in nm * nm. If this parameter is set, 'Area per lipid' will only describe the lipids in the lower leaflet.",
                    "class": "advanced"
                },
                { 
                    "id": "parameter_asym",
                    "description": "Membrane asymmetry (number of lipids)",
                    "input_type": "number",
                    "min": "",
                    "max": "",
                    "step": "1",
                    "default": "",
                    "placeholder": "e.g. 5, -20",
                    "information": "Creates an asymmetric amount of lipids in the upper and lower membrane leaflets. Positive values will result in more lipids in the lower membrane, negative values will create more lipids in the upper membrane.",
                    "class": "advanced"

                },
                { 
                    "id": "parameter_hole",
                    "description": "Hole size",
                    "input_type": "number",
                    "min": "0",
                    "max": "",
                    "step": "0.1",
                    "default": "",
                    "placeholder": "e.g. 4",
                    "information": "Creates a hole with the given radius in nm. Can be used in combination with 'disc size'.",
                    "class": "advanced"

                },
                { 
                    "id": "parameter_disc",
                    "description": "Disc size",
                    "input_type": "number",
                    "min": "0",
                    "max": "",
                    "step": "0.1",
                    "default": "",
                    "placeholder": "e.g. 5",
                    "information": "Creates a disc (opposite of hole) with the given radius in nm. Can be used in combination with 'hole size'.",
                    "class": "advanced"

                },
                { 
                    "id": "parameter_bd",
                    "description": "Bead distance unit",
