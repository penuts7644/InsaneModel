        { 
            "id": "gridsizing_menu",
            "name": "Grid Sizing",
            "expanded": "collapse in",
            "inputs": [
                { 
                    "id": "parameter_d",
                    "description": "Distance between periodic images",
                    "input_type": "number",
                    "min": "1.0",
                    "max": "25.0",
                    "step": "0.5",
                    "default": "",
                    "placeholder": "e.g. 10",
                    "information": "Size of the simulation in nanometer, for the x, y and z axes. This value must be between 1 and 25, the default value is 10.",
                    "class": "standard"
                },
                {
                    "id": "parameter_x",
                    "description":"Distance between periodic images in direction x",
                    "input_type": "number",
                    "min": "1.0",
                    "max": "25.0",
                    "step": "0.5",
                    "default": "",
                    "placeholder": "e.g. 10",
                    "information": "Size of the simulation in nanometer, for the x axis. This value must be between 1 and 25.",
                    "class": "advanced"
                },
                {
                    "id": "parameter_y",
                    "description":"Distance between periodic images in direction y",
                    "input_type": "number",
                    "min": "1.0",
                    "max": "25.0",
                    "step": "0.5",
                    "default": "",
                    "placeholder": "e.g. 10",
                    "information": "Size of the simulation in nanometer, for the y axis. This value must be between 1 and 25.",
                    "class": "advanced"
                },
                {
                    "id": "parameter_z",
                    "description":"Distance between periodic images in direction z",
                    "input_type": "number",
                    "min": "1.0",
                    "max": "25.0",
                    "step": "0.5",
                    "default": "",
                    "placeholder": "e.g. 10",
                    "information": "Size of the simulation in nanometer, for the z axis. This value must be between 1 and 25.",
                    "class": "advanced"
                },
                {
                    "id": "parameter_dz",
                    "description": "Use z-distance plus protein size for z",
                    "input_type": "radio",
                    "default": "false",
                    "information": "If z is given: z is replaced by z + protein size.",
                    "class": "advanced",
                    "options": [
                        {
                            "label" : "Yes",
                            "value" : "true"
                        },
                        {
                            "label" : "No",
                            "value" : "false"
                        }
                    ]
                },
                {
                    "id": "parameter_pbc",
                    "description":"Periodic boundary condition",
                    "input_type": "select",
                    "default": "",
                    "information": "The shape of the unit cell (note that this does not affect the image created by JSmol)",
                    "class": "advanced",
                    "options": [
                        {
                            "label":"Hexagonal",
                            "value" : "hexagonal"
                        },
                        {
                            "label" : "Rectangular",
                            "value" : "rectangular"
                        },
                        {
                            "label" : "Square",
                            "value" : "square"
                        },
                        {
                            "label" : "Cubic",
                            "value" : "cubic"
                        },
                        {
                            "label" : "Optimal",
                            "value" : "optimal"
                        },
                        {
                            "label" : "Keep",
                            "value" : "keep"
                        }
                    ]
                }
            ]
